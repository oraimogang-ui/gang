    {
      "parameters": {
        "url": "={{$env.ROOMRESERVE_BASE_URL || 'http://localhost:8080'}}/api/rooms/available",
        "queryParametersUi": {
          "parameter": [
            {
              "name": "start",
              "value": "={{$json.checkIn || ''}}"
            },
            {
              "name": "end",
              "value": "={{$json.checkOut || ''}}"
            }
          ]
        },
        "options": {
          "responseFormat": "json"
        }
      },
      "id": "3fb21dd7-f4c1-41c6-8363-334bf076046b",
      "name": "RoomReserve Availability",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        1880,
        220
      ]
    },
    {
      "parameters": {
        "functionCode": "// Check for availability of the requested category.\nconst requested = ($json.requestedRoom || 'standard').toLowerCase();\nconst available = ($json.availableRooms) || ($json[0]?.availableRooms) || [];\nconst match = available.find(r => (r.name || r.roomType || '').toLowerCase().includes(requested));\nif (!match) {\n  return [{ status: 'unavailable', intent: 'availability', response: `Sorry, ${requested} is not available for those dates. Can we try different dates or another room type?` }];\n}\nreturn [{ status: 'available', room: match, intent: 'booking', requestedRoom: requested, response: 'Room available, proceeding to guest capture.' }];"
      },
      "id": "7dea235e-932a-42ff-8b6a-f36f5738c599",
      "name": "Validate Availability",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        2080,
        220
      ]
    },
    {
      "parameters": {
        "conditions": {
          "string": [
            {
              "value1": "={{$json.status}}",
              "operation": "equal",
              "value2": "available"
            }
          ]
        }
      },
      "id": "f726cb9c-15f9-4f43-acb4-bc815a9e0615",
      "name": "Availability OK?",
