    {
      "parameters": {
        "functionCode": "const caller = $items('Geocode Caller')[0]?.json?.body?.[0] || $items('Geocode Caller')[0]?.json?.[0];\nconst hotel = $json.body?.[0] || $json[0] || $items('Geocode Hotel')[0]?.json?.body?.[0];\nreturn [{ callerLat: caller?.lat, callerLon: caller?.lon, hotelLat: hotel?.lat, hotelLon: hotel?.lon, intent: 'directions' }];"
      },
      "id": "486357c3-44e3-4c3c-9554-ade05c88cb5a",
