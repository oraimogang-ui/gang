      "id": "d120c6cc-b693-4d70-9772-c1b2ad1c9257",
      "name": "OSRM Route",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        2480,
        1020
      ]
    },
    {
      "parameters": {
        "functionCode": "const data = $json.routes ? $json : ($json.body || {});\nconst legs = data.routes?.[0]?.legs || [];\nconst steps = legs.flatMap(l => l.steps || []);\nconst directions = steps.map((s, i) => `${i + 1}. ${s.maneuver?.instruction || s.name || 'Continue'}`);\nreturn [{ response: directions.join('\\n') || 'Directions unavailable right now. Please try again.', intent: 'directions' }];"
      },
      "id": "56aa2dd4-6731-4927-874e-d31935d294e4",
      "name": "Format Directions",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        2680,
        1020
      ]
    },
    {
      "parameters": {
        "functionCode": "return [{ response: 'I want to make sure I understand. Are you asking about booking, availability, room details, the menu, directions, or something else? I can also transfer you to a human if needed.', intent: 'fallback' }];"
      },
      "id": "1ee4b4ff-36f2-48ff-9a7f-67f682da4458",
      "name": "Fallback Clarify",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
