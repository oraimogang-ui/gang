        },
        "jsonParameters": true,
        "authentication": "basicAuth",
        "sendBody": true,
        "body": "={\n  \"guestId\": $json.body.id || $json.id,\n  \"roomType\": $json.requestedRoom,\n  \"startDate\": $json.checkIn,\n  \"endDate\": $json.checkOut\n}"
      },
      "id": "588664b3-5dca-4689-8c26-49b361ad89aa",
      "name": "Create Reservation",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        2680,
        180
      ]
    },
    {
      "parameters": {
        "functionCode": "// Build booking summary\nconst catalog = $json.roomCatalog || {};\nconst requested = ($json.requestedRoom || '').toLowerCase();\nconst matchKey = Object.keys(catalog).find(k => k.toLowerCase() === requested) || $json.requestedRoom;\nconst price = (matchKey && catalog[matchKey]?.price) || $json.room?.price || 'per-night rate';\nconst reservationId = $json.body?.id || $json.id || 'pending';\nreturn [{\n  response: `Reservation confirmed! ${$json.guestName || 'Guest'} booked a ${matchKey || requested} from ${$json.checkIn} to ${$json.checkOut}. Rate: \u20a6${price}. Reservation ID: ${reservationId}. Please pay on arrival.`,\n  reservationId,\n  intent: 'booking'\n}];"
      },
      "id": "6a739a6b-ae41-4962-a78d-88db455880bf",
      "name": "Booking Summary",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
      "position": [
        2880,
        180
      ]
    },
    {
      "parameters": {
        "functionCode": "// Format room info by type\nconst catalog = $json.roomCatalog || {};\nconst parts = Object.entries(catalog).map(([name, data]) => `${name}: \u20a6${data.price} per night. Amenities: ${data.amenities.join(', ')}`);\nreturn [{ response: parts.join('\\n'), intent: 'room_info' }];"
      },
      "id": "807e57d3-2c1d-449d-88c2-75e2dd2e44af",
      "name": "Room Info Response",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
