    {
      "parameters": {
        "operation": "execute",
        "database": "={{$env.AUDIT_DB_PATH || '/home/node/.n8n/mambillah-audit.sqlite'}}",
        "statement": "CREATE TABLE IF NOT EXISTS audit (id INTEGER PRIMARY KEY AUTOINCREMENT, created_at TEXT, intent TEXT, transcript TEXT, response TEXT); INSERT INTO audit (created_at, intent, transcript, response) VALUES (datetime('now'), :intent, :transcript, :response);",
        "values": {
          "namedValues": [
            {
