    {
      "parameters": {
        "functionCode": "// Lightweight keyword-based intent detection.\nconst text = ($json.transcript || '').toLowerCase();\nconst intents = [\n  { key: 'booking', keywords: ['book', 'reserve'] },\n  { key: 'availability', keywords: ['available', 'vacancy', 'vacancies', 'free room'] },\n  { key: 'room_info', keywords: ['room', 'suite', 'deluxe', 'standard', 'price', 'amenities'] },\n  { key: 'menu', keywords: ['menu', 'restaurant', 'bar', 'food', 'drink'] },\n  { key: 'club', keywords: ['club', 'club-m', 'dj', 'music'] },\n  { key: 'gym', keywords: ['gym', 'fitness'] },\n  { key: 'directions', keywords: ['direction', 'route', 'how do i get', 'map'] }\n];\nlet intent = 'fallback';\nfor (const item of intents) {\n  if (item.keywords.some(k => text.includes(k))) {\n    intent = item.key;\n    break;\n  }\n}\nreturn [{ ...$json, intent }];"
      },
      "id": "86dbc0ce-8b79-4c7f-bfb9-76126d4b260d",
      "name": "Detect Intent",
      "type": "n8n-nodes-base.function",
      "typeVersion": 1,
